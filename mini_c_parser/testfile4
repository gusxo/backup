#include<stdio.h>
#include"custom.h"

int factorial(int n){
    if(n <= 1)
        return 1;
    return n * factorial(n-1);
}

int main(){
    int n;
    scanf("%d", &n);
    if(n < 0)
        if(1){
            if(1==1)
                printf("input error\n");
        }
    if(n>=0){
        int f = factorial(n);
        printf("factorial(%d) : %d\n", n, f);
    }
    return 0;
}